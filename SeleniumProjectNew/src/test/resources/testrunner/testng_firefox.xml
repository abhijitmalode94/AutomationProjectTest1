<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNG Regression Suite" verbose="4" thread-count="3" parallel="tests">


	<test name="Google Test">

		<parameter name="url" value="https://www.google.com/" />
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="TestNg_session.GoogleTest" />
		</classes>
	</test>


	<test name="Amazone Test">

		<parameter name="url" value="https://www.amazon.in/" />
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="TestNg_session.AmazoneTest" />
		</classes>
	</test>


	<test name="OpenCart Test">

		<parameter name="url" value="https://naveenautomationlabs.com/opencart/" />
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="TestNg_session.OpenCartStoreTest" />
		</classes>
	</test>

</suite>